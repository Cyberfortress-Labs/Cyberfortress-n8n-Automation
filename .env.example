# Example environment variables for local development
# Copy to .env and edit values as needed

# Host config used by docker-compose labels and n8n
SUBDOMAIN=n8n
DOMAIN_NAME=cyberfortress.local

# Timezone
GENERIC_TIMEZONE=Asia/Ho_Chi_Minh

# n8n auth (change in production!)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin@n8n.local
N8N_BASIC_AUTH_PASSWORD=test

# Optional: override protocol and host for local testing
# N8N_PROTOCOL=http
# N8N_HOST=localhost
# WEBHOOK_URL=http://localhost:5678/
